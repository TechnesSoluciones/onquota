{
  "name": "onquota-aws-infrastructure",
  "version": "1.0.0",
  "description": "OnQuota AWS Infrastructure as Code using CDK",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context environment=dev",
    "deploy:staging": "cdk deploy --all --context environment=staging",
    "deploy:prod": "cdk deploy --all --context environment=production",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "destroy": "cdk destroy --all",
    "bootstrap": "cdk bootstrap",
    "test": "jest",
    "lint": "eslint lib/**/*.ts bin/**/*.ts"
  },
  "keywords": [
    "aws",
    "cdk",
    "infrastructure",
    "iac",
    "onquota"
  ],
  "author": "OnQuota Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "aws-cdk": "^2.115.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-cdk/aws-servicecatalogappregistry-alpha": "^2.115.0-alpha.0",
    "aws-cdk-lib": "^2.115.0",
    "constructs": "^10.3.0",
    "dotenv": "^16.3.1",
    "source-map-support": "^0.5.21"
  }
}
